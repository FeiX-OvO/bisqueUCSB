<?xml version="1.0" encoding="utf-8"?>
<module name="3DMaterialGAN" type="runtime">

    <!-- Comments are OK -->
    <tag name="inputs">
        <tag name="resource_url" type="resource">
            <template>
                <tag name="accepted_type" value="tarfile" />
                <tag name="label" value="Saved Weights" />
                <tag name="fail_message" value="Requires .tar file"  />
            </template>
        </tag>

        <tag name="mex_url"  type="system-input" />
        <tag name="bisque_token"  type="system-input" />
    </tag>

    <tag name="outputs">
         <tag name="metadata" type="tag">
            <template>
                <tag name="label" value="Generated binvox file" />
            </template>
         </tag>
    </tag>

    <tag name="execute_options">
        <tag name="iterable" value="image_url" type="dataset" />
        <!-- Example for a blocked iteration -->
        <tag name="blocked_iter" value="true" />
    </tag>

    <tag name="module_options" >
        <tag name="version" value="1" />
    </tag>

    <tag name="display_options" >
       <tag name="group" value="Examples" />
    </tag>

    <tag name="interface">
        <tag name="javascript" type="file" value="webapp.js" />
        <tag name="css" type="file" value="webapp.css" />
    </tag>
    
    <tag name="help" type="file" value="public/help.html" />
    <tag name="thumbnail" type="file" value="public/thumbnail.png" />

    <tag name="title" type="string" value="3DMaterialGAN" />
    <tag name="authors" type="string" value="Devendra Jangid" />
    <tag name="description" type="string" value="This module generates a single grain from latent space vector." />
</module>
